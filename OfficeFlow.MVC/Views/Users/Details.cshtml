        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Street)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Street)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.HouseNumber)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.HouseNumber)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ApartmentNumber)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ApartmentNumber)
        </dd>
    </dl>
</div>
<div>
    <td>
        <a class="btn btn-outline-warning" asp-action="Index">Wróć do listy</a>
    </td>
    <td>
        <a class="btn btn-outline-dark" asp-area="" asp-controller="Users" asp-action="Edit" asp-route-publicId="@Model.PublicId">Edytuj</a>
    </td>
</div>
